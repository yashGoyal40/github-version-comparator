{
  "name": "github-version-comparator",
  "version": "1.0.11",
  "description": "A modern, client-side GitHub version comparator with Docker & Kubernetes support. Compare any repository versions with beautiful diffs, commit history, and file changes.",
  "keywords": [
    "github",
    "version-comparison",
    "diff",
    "git",
    "docker",
    "kubernetes",
    "nextjs",
    "typescript",
    "client-side",
    "static-hosting"
  ],
  "author": "yashGoyal40",
  "license": "MIT",
  "homepage": "https://github.com/yashGoyal40/github-version-comparator#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/yashGoyal40/github-version-comparator.git"
  },
  "bugs": {
    "url": "https://github.com/yashGoyal40/github-version-comparator/issues"
  },
  "private": false,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build && node scripts/post-build.js",
    "start": "next start",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "type-check": "tsc --noEmit",
    "deploy": "vercel --prod",
    "version:patch": "npm version patch && git push --follow-tags",
    "version:minor": "npm version minor && git push --follow-tags",
    "version:major": "npm version major && git push --follow-tags",
    "release": "npm run build && npm run deploy"
  },
  "dependencies": {
    "autoprefixer": "^10.4.21",
    "lucide-react": "^0.544.0",
    "next": "15.5.3",
    "postcss": "^8.5.6",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "tailwindcss": "^3.4.17"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.5.3",
    "typescript": "^5"
  }
}
